{
  "name": "Task Executer",
  "description": "Executing predefined local programs",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Download Youtube audio"
  },
  "manifest_version": 2,
  "icons": {
    "512": "taskexecuter-mp3.png"
  }
}
