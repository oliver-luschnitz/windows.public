{
    "name": "Task Executer III",
    "description": "Executing predefined local programs",
    "version": "3.0",
    "manifest_version": 3,
    "icons": {
        "128": "taskexecuter.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "action": {
        "default_title": "Execute local tasks",
        "default_popup": "popup.html"
    }
}